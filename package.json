{
  "name": "vue-shop-admin",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/sartim/vue_shop_admin"
  },
  "license": "MIT",
  "scripts": {
    "build": "vue-cli-service build",
    "build:dev": "webpack --display-error-details --config webpack.config.js --progress --color",
    "build:prod": "webpack --env.prod --config webpack.config.js --progress --color",
    "start": "webpack-dev-server --open",
    "lint": "vue-cli-service lint",
    "test": "mocha-webpack -w --webpack-config webpack.config.js --require test/setup.js test/**/*.spec.js",
    "watch": "webpack -w -d"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.4.3",
    "crypto-js": "^3.1.9-1",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vue-socket.io": "^3.0.7",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^4.4.5",
    "@vue/cli-service": "^3.7.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-vue": "^2.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^0.28.11",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "expect": "^24.8.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^15.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.1.4",
    "mocha-webpack": "^2.0.0-beta.0",
    "path": "^0.12.7",
    "vue-loader": "^14.2.4",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.15.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
